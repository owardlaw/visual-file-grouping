{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Extension",
        "type": "extensionHost",
        "request": "launch",
        "runtimeExecutable": "${execPath}",
        "args": [
          "--extensionDevelopmentPath=${workspaceFolder}"
        ],
        "outFiles": [
          "${workspaceFolder}/out/**/*.js"
        ],
        "preLaunchTask": "compile",
        "sourceMaps": true,
        "stopOnEntry": false,
        "sourceMapPathOverrides": {
          "webpack:///src/*": "${workspaceFolder}/src/*"
        }
      }
    ]
  }
  